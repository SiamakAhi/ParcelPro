
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="fa" dir="rtl">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <!-- ===============================================-->
    <!--    Document Title-->
    <!-- ===============================================-->
    <title>قالب پنل مدیریتی فالکون HTML | پیشخوان &amp; اپلیکیشن تحت وب</title>


    <!-- ===============================================-->
    <!--    Favicons-->
    <!-- ===============================================-->
    <link rel="apple-touch-icon" sizes="180x180" href="~/lib/falcon/img/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/lib/falcon/img/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/lib/falcon/img/favicons/favicon-16x16.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/lib/falcon/img/favicons/favicon.ico">
    <link rel="manifest" href="~/lib/falcon/img/favicons/manifest.json">
    <meta name="msapplication-TileImage" content="~/lib/falcon/img/favicons/mstile-150x150.png">
    <meta name="theme-color" content="#ffffff">
    <script src="~/lib/falcon/js/config.js"></script>
    <script src="~/lib/falcon/vendors/overlayscrollbars/OverlayScrollbars.min.js"></script>


    <!-- ===============================================-->
    <!--    Stylesheets-->
    <!-- ===============================================-->
    <!--    <link rel="preconnect" href="https://fonts.gstatic.com">-->
    <!--    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,500,600,700%7cPoppins:300,400,500,600,700,800,900&amp;display=swap" rel="stylesheet">-->
    <link href="~/lib/falcon/vendors/overlayscrollbars/OverlayScrollbars.min.css" rel="stylesheet">
    <link href="~/lib/falcon/css/fonts.css" rel="stylesheet">
    <link href="~/lib/falcon/css/theme-rtl.min.css" rel="stylesheet" id="style-rtl">
    <link href="~/lib/falcon/css/theme.min.css" rel="stylesheet" id="style-default">
    <link href="~/lib/falcon/css/user-rtl.min.css" rel="stylesheet" id="user-style-rtl">
    <link href="~/lib/falcon/css/user.min.css" rel="stylesheet" id="user-style-default">
    <link href="~/lib/falcon/css/flatpickr-persian.css" rel="stylesheet">
    <script>
        var isRTL = JSON.parse(localStorage.getItem('isRTL'));
        if (isRTL) {
            var linkDefault = document.getElementById('style-default');
            var userLinkDefault = document.getElementById('user-style-default');
            linkDefault.setAttribute('disabled', true);
            userLinkDefault.setAttribute('disabled', true);
            document.querySelector('html').setAttribute('dir', 'rtl');
        } else {
            var linkRTL = document.getElementById('style-rtl');
            var userLinkRTL = document.getElementById('user-style-rtl');
            document.querySelector('html').setAttribute('dir', 'ltr');
            linkRTL.setAttribute('disabled', true);
            userLinkRTL.setAttribute('disabled', true);
        }
    </script>
</head>


<body>

    <!-- ===============================================-->
    <!--    Main Content-->
    <!-- ===============================================-->
    <main class="main" id="top">
        <div class="container-fluid">
            <div class="row min-vh-100 flex-center g-0">
                <div class="col-lg-8 col-xxl-5 py-3 position-relative">
                    <img class="bg-auth-circle-shape" src="~/lib/falcon/img/icons/spot-illustrations/bg-shape.png" alt="" width="250"><img class="bg-auth-circle-shape-2" src="~/lib/falcon/img/icons/spot-illustrations/shape-1.png" alt="" width="150">
                    <div class="card overflow-hidden z-index-1">
                        <div class="card-body p-0">
                            <div class="row g-0 h-100">
                                <div class="col-md-5 text-center bg-card-gradient">
                                    <div class="position-relative p-4 pt-md-5 pb-md-7 light">
                                        <div class="bg-holder bg-auth-card-shape" style="background-image:url(~/lib/falcon/img/icons/spot-illustrations/half-circle.png);"></div>
                                        <!--/.bg-holder-->
                                        <div class="z-index-1 position-relative">
                                            <a class="link-light mb-4 font-sans-serif fs-4 d-inline-block fw-bolder" href="~/lib/falcon/index.html">گارنِت</a>
                                            <p class="opacity-75 text-white">با سیستم تیکتینگ <strong>گارنِت</strong> بهترین پشتیبانی را در سریعترین زمان ممکن دریافت کنید.</p>
                                        </div>
                                    </div>
                                    <div class="mt-3 mb-4 mt-md-4 mb-md-5 light">
                                        <p class="pt-3 text-white">حساب کاربری دارید؟<br><a class="btn btn-outline-light mt-2 px-4" asp-action="Login" asp-controller="usermanager" asp-area="">وارد شوید</a></p>
                                    </div>
                                </div>
                                <div class="col-md-7 d-flex flex-center">
                                    <div class="p-4 p-md-5 flex-grow-1">
                                        <h3>ثبت نام</h3>
                                        <form>
                                            <div class="mb-3"><label class="form-label" for="card-name">نام</label><input class="form-control" type="text" autocomplete="on" id="card-name"></div>
                                            <div class="mb-3"><label class="form-label" for="card-email">آدرس ایمیل</label><input class="form-control" type="email" autocomplete="on" id="card-email"></div>
                                            <div class="row gx-2">
                                                <div class="mb-3 col-sm-6"><label class="form-label" for="card-password">کلمه عبور</label><input class="form-control" type="password" autocomplete="on" id="card-password"></div>
                                                <div class="mb-3 col-sm-6"><label class="form-label" for="card-confirm-password">رمز عبور را تایید کنید</label><input class="form-control" type="password" autocomplete="on" id="card-confirm-password"></div>
                                            </div>
                                            <div class="form-check"><input class="form-check-input" type="checkbox" id="card-register-checkbox"><label class="form-label" for="card-register-checkbox"><a href="#!">من شرایط</a> و <a href="#!">سیاست حفظ حریم خصوصی</a> را می پذیرم<a href="#!"></a></label></div>
                                            <div class="mb-3"><button class="btn btn-primary d-block w-100 mt-3" type="submit" name="submit">ثبت نام</button></div>
                                        </form>
                                        <div class="position-relative mt-4">
                                            <hr class="bg-300">
                                            <div class="divider-content-center">یا ثبت نام کنید</div>
                                        </div>
                                        <div class="row g-2 mt-2">
                                            <div class="col-sm-6"><a class="btn btn-outline-google-plus btn-sm d-block w-100" href="#"><span class="fab fa-google-plus-g me-2" data-fa-transform="grow-8"></span>گوگل</a></div>
                                            <div class="col-sm-6"><a class="btn btn-outline-facebook btn-sm d-block w-100" href="#"><span class="fab fa-facebook-square me-2" data-fa-transform="grow-8"></span>فیس بوک</a></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>
    <!-- ===============================================-->
    <!--    End of Main Content-->
    <!-- ===============================================-->




    <!-- ===============================================-->
    <!--    JavaScripts-->
    <!-- ===============================================-->
    <script src="~/lib/falcon/vendors/popper/popper.min.js"></script>
    <script src="~/lib/falcon/vendors/bootstrap/bootstrap.min.js"></script>
    <script src="~/lib/falcon/vendors/anchorjs/anchor.min.js"></script>
    <script src="~/lib/falcon/vendors/is/is.min.js"></script>

    <script src="~/lib/falcon/js/momenjs.js"></script>
    <script src="~/lib/falcon/js/moment-range.js"></script>
    <script src="~/lib/falcon/js/flatpickr-persian.js"></script>
    <script src="~/lib/falcon/js/flatpickr-persian-fa.js"></script>

    <script src="~/lib/falcon/js/dayjs.local-fa.js"></script>
    <script src="~/lib/falcon/vendors/dayjs/dayjs.min.js"></script>
    <script src="~/lib/falcon/js/jdate.min.js"></script>
    <script>window.Date = window.JDate;</script>

    <script src="~/lib/falcon/vendors/fontawesome/all.min.js"></script>
    <script src="~/lib/falcon/vendors/lodash/lodash.min.js"></script>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=window.scroll"></script>
    <script src="~/lib/falcon/vendors/list.js/list.min.js"></script>
    <script src="~/lib/falcon/js/theme.js"></script>

</body>

</html>
